<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	<typeAliases>
		<typeAlias type="ga.bowwow.service.user.UserAccount" alias="userAccount"/>
	<!--
		<typeAlias type="com.spring.biz.board.BoardVO" alias="board"/>
		-->
		<typeAlias type="ga.bowwow.user.UserAccount" alias="UserAccount"/>
		<typeAlias type="ga.bowwow.board.Board" alias="Board"/>
		<typeAlias type="ga.bowwow.pet.Pet" alias="pet"/>
		<typeAlias type="ga.bowwow.store.Cartlist" alias="storeCartlist"/>
		<typeAlias type="ga.bowwow.store.Comm" alias="productComm"/>
		<typeAlias type="ga.bowwow.store.Inquiry" alias="storeInquiry"/>
		<typeAlias type="ga.bowwow.store.Order" alias="storeOrder"/>
		<typeAlias type="ga.bowwow.store.Products" alias="storeProducts"/>
		<typeAlias type="ga.bowwow.store.Response" alias="storeInquiryResponse"/>
		<typeAlias type="ga.bowwow.store.Review" alias="productReview"/>

	</typeAliases>

  <mappers>
  	<mapper resource="mappings/UserMapper.xml"/>
    <!--
    <mapper resource=""/>
     -->
    <mapper resource="mappings/UserMapper.xml"/>
    <mapper resource="mappings/BoardMapper.xml"/>
    <mapper resource="mappings/PetMapper.xml"/>
    <mapper resource="mappings/ProductsReviewMapper.xml"/>
    <!--
    <mapper resource="mappings/StoreMapper.xml"/>
     -->
    <!--
    <mapper resource="mappings/StoreInquiryMapper.xml"/>
     -->
  </mappers>
</configuration>
