<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	<typeAliases>

		<typeAlias type="ga.bowwow.service.board.Board" alias="Board" />
		<typeAlias type="ga.bowwow.service.pet.Pet" alias="pet" />
		<typeAlias type="ga.bowwow.service.store.Product" alias="Product" />
		<typeAlias type="ga.bowwow.service.store.Review" alias="Review" />
		<typeAlias type="ga.bowwow.service.store.CartList" alias="CartList" />
		<typeAlias type="ga.bowwow.service.store.Order" alias="Order" />
		<typeAlias type="ga.bowwow.service.store.Inquiry" alias="Inquiry" />

	</typeAliases>

	<mappers>
		<mapper resource="mappings/BoardMapper.xml" />
		<mapper resource="mappings/PetMapper.xml" />
		<mapper resource="mappings/StoreMapper.xml" />
		<mapper resource="mappings/CartListMapper.xml" />
		<mapper resource="mappings/StoreReviewMapper.xml" />
		<mapper resource="mappings/StoreInquiryMapper.xml" />
	</mappers>
</configuration>